<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
 xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="object_buttons" meta_type="CMF Action Category">
  <object name="imio.faceted.disable" meta_type="CMF Action" i18n:domain="imio.facetednavigation">
   <property name="title"
      i18n:translate="">Disable faceted navigation</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${object/absolute_url}/@@faceted_imio_subtyper/disable</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property
      name="available_expr">object/@@faceted_imio_subtyper/can_disable</property>
   <property name="permissions">
    <element value="imio.facetednavigation: Configure faceted"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="imio.faceted.enable" meta_type="CMF Action" i18n:domain="imio.facetednavigation">
   <property name="title"
      i18n:translate="">Enable faceted navigation</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${object/absolute_url}/@@faceted_imio_subtyper/enable</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property
      name="available_expr">object/@@faceted_imio_subtyper/can_enable</property>
   <property name="permissions">
    <element value="imio.facetednavigation: Configure faceted"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>
